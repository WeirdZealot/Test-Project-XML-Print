<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Test_Project_XML_Print"
             x:Name="DocViewer1" x:Class="Test_Project_XML_Print.DocViewer"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <XmlDataProvider x:Name="Viewer" x:Key="provider" Source= "test.xml"></XmlDataProvider>
    </UserControl.Resources>

    <Grid x:Name="Grid">
        <WebBrowser x:Name="browser" HorizontalAlignment="Left" Height="450" VerticalAlignment="Top" Width="800"/>
    </Grid>
</UserControl>
